generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Picture {
  id          Int    @id @default(autoincrement())
  copyright   String
  date        String @unique
  explanation String
  hdUrl       String
  title       String
  url         String
}
